    "import pandas as pd\n",
    "from google import genai\n",
    "import re\n",
    "import time\n",
    "import random\n",
    "from google.api_core.retry import Retry"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cfc7936c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def main():\n",
    "    # =========================\n",
    "    # Step 1: Initialize Gemini\n",
    "    # =========================\n",
    "    client = genai.Client(api_key=\"")  # Replace with your actual API key\n",
    "\n",
    "    # =========================\n",
    "    # Step 2: Load Data\n",
    "    # =========================\n",
    "    founders = pd.read_csv(\"founders.csv\")\n",
    "    investors = pd.read_csv(\"investors.csv\")\n",
    "\n",
    "    # =========================\n",
    "    # Step 3: Select ONE Founder\n",
    "    # =========================\n",
    "    founder = founders.iloc[0]\n",
    "    founder_info = founder.astype(str).to_dict()\n",
    "    founder_name = re.sub(r'^=\"|\"$', '', founder.name).strip()\n",
    "    founder_matches = []\n",
    "\n",
    "    print(f\"üöÄ Matching for Founder: {founder_name}\")\n",
    "\n",
    "    # =========================\n",
    "    # Step 4: Match All Investors to the Selected Founder\n",
    "    # =========================\n",
    "    MAX_RETRIES = 5\n",
    "    BASE_DELAY = 2\n",
    "\n",
    "    for _, investor in investors.iterrows():\n",
    "        investor_info = investor.astype(str).to_dict()\n",
    "        investor_name = investor_info.get('Name', 'Unknown Investor')\n",
    "\n",
    "        print(f\"‚û°Ô∏è Processing: {founder_name} √ó {investor_name}\")\n",
    "\n",
    "        prompt = f\"\"\"\n",
    "        You are a professional startup-investor match analyst. Based on the following founder and investor information, determine their compatibility and return a match score from 0 to 100. Only return the score in this format:\n",
    "\n",
    "        Match Score: XX\n",
    "\n",
    "        Founder Info:\n",
    "        {founder_info}\n",
    "\n",
    "        Investor Info:\n",
    "        {investor_info}\n",
    "        \"\"\"\n",
    "\n",
    "        for attempt in range(MAX_RETRIES):\n",
    "            try:\n",
    "\n",
    "\n",
    "                response = client.models.generate_content(\n",
    "                    model=\"gemini-2.0-flash\",\n",
    "                    contents=prompt\n",
    "\n",
    "                )\n",
    "\n",
    "\n",
    "                text = response.text\n",
    "\n",
    "                match = re.search(r\"Match Score[:\\s]*(\\d+)\", text)\n",
    "                score = int(match.group(1)) if match else -1\n",
    "\n",
    "                founder_matches.append({\n",
    "                    \"Founder\": founder_name,\n",
    "                    \"Investor\": investor_name,\n",
    "                    \"Match Score\": score\n",
    "                })\n",
    "\n",
    "                time.sleep(random.uniform(1.5, 2.5))\n",
    "                break\n",
    "\n",
    "            except Exception as e:\n",
    "                print(f\"‚ö†Ô∏è Attempt {attempt + 1} failed for {founder_name} √ó {investor_name}: {e}\")\n",
    "        if investor_name == \"Zvi Schreiber\":\n",
    "            break\n",
    "\n",
    "    # =========================\n",
    "    # Step 5: Save Ranked Results for the Single Founder\n",
    "    # =========================\n",
    "    sorted_matches = sorted(founder_matches, key=lambda x: x[\"Match Score\"], reverse=True)\n",
    "    result_df = pd.DataFrame(sorted_matches)\n",
    "    output_file = f\"{founder_name.replace(' ', '_')}_investor_matches.csv\"\n",
    "    result_df.to_csv(output_file, index=False)\n",
    "\n",
    "    print(f\"‚úÖ Matching complete. Results saved to '{output_file}'\")"
   ]
  },
